<div id="wiki-wrapper" class="page">
<div id="head">
  <h1>{{title}}</h1>
  <ul class="actions">
    <li class="minibutton"><a href="/pages"
      class="action-all-pages">All Pages</a></li>
    <li class="minibutton" class="jaws">
      <a href="#" id="minibutton-new-page">New Page</a></li>
      <li class="minibutton" class="jaws">
      <a href="#" id="minibutton-upload-page">Upload Attachment</a></li>
    <li class="minibutton"><a href="/edit/{{escaped_name}}"
       class="action-edit-page">Edit Page</a></li>
    <li class="minibutton"><a href="/history/{{escaped_name}}"
       class="action-page-history">Page History</a></li>
  </ul>
  {{>searchbar}}
</div>
<div id="wiki-content">
  <div class="wrap{{#has_footer}} has-footer{{/has_footer}}{{#has_sidebar}} has-rightbar{{/has_sidebar}}">
  <div id="wiki-body" class="gollum-{{format}}-content">
    <div id="template">
      {{{content}}}
    </div>
  </div>
  <div id="wiki-rightbar" class="gollum-{{sidebar_format}}-content">
    {{{sidebar_content}}}
    <hr>
	<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js"></script>
	<script type="text/javascript" src="http://dev1.makerpress.com:8080/script.js"></script>
	<script type="text/javascript">
		jQuery(function () {
			$('#progress').file_progress();
		});
	</script>
        <ul class="actions">
           <li class="minibutton">
              <a id="start_button" href="#" onclick="return false;">Build</a>
           </li>
           <li class="minibutton">
              <a href="http://dev1.makerpress.com:8080/epub/book.epub" target="_blank">EPUB</a>
           </li>
           <li class="minibutton">
              <a href="http://dev1.makerpress.com:8080/log/log.html" target="_blank">Log</a>
           </li>
         </ul>
        <div id="progress" class="progress_log">
           <div>
              <span class="status">Build Log</span>
	   </div>
	   <ul class="log"></ul>
        </div>

  </div>
  {{#has_footer}}
  <div id="wiki-footer" class="gollum-{{footer_format}}-content">
    <div id="footer-content">
      {{{footer_content}}}
    </div>
  </div>
  {{/has_footer}}
  </div>

</div>
<div id="footer">
  <p id="last-edit">Last edited by <b>{{author}}</b>, {{date}}</p>
</div>
</div>

<script type="text/javascript" src="/javascript/MathJax/MathJax.js"></script>
